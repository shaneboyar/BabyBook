{
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "eslint": "eslint . --ext .tsx,.ts",
    "prettier": "prettier --check *.ts*",
    "lint": "npm-run-all --parallel eslint prettier",
